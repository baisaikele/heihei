<?xml version="1.0" encoding="utf-8"?>
<com.base.widget.RoundFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="@dimen/t60dp"
    android:layout_height="@dimen/t60dp"
    android:alpha="0.9"
    android:background="#212427"
    app:corner_radius_all="120dp" >

    <RelativeLayout
        android:layout_width="@dimen/t60dp"
        android:layout_height="@dimen/t60dp"
        android:background="@color/hh_color_d" >
        <ImageView
            android:id="@+id/iv_loading"
            android:layout_width="@dimen/t40dp"
            android:layout_height="@dimen/t40dp"
            android:layout_centerInParent="true"
            android:visibility="gone" />
    </RelativeLayout>

</com.base.widget.RoundFrameLayout>